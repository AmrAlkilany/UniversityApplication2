@model List<Department>

<center>
    <table class="table table-bordered text-center table-hover">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Manager</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Manager</td>
            </tr>
        }
    </table>
</center>